
public class LoopDemo {

	public static void main(String[] args) {
		// A ~ Z 까지 출력하기 
		int i = 65; // 초기화 initialization  /1번
		for ( ; i<=90; ) {   //2번
			
			if (i / 5 > 0 ) {
				System.out.printf("%c\n",(char) i );
				
			} else {
				System.out.printf("%c\t",(char) i );
			}
			i++; 
			
		}
		System.out.println();

	}

}



//1부터 100까지 4의 배수는 몇개인가. 