
public class LoopDemo {

	public static void main(String[] args) {

		//1부터 100까지 4의 배수는 몇개인가. 
		int count =0;
		for (int i =1 ; i <=100; i++) {
			if(i % 4 == 0 ) {
				count ++;
			}
		}
		System.out.printf( "갯수는 %d\n", count);
		
		
		
		
		
		
	}
}
